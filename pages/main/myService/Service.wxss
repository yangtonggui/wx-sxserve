/* pages/main/myService/Service.wxss */
.main{
  width: 100%;
  min-height: 100vh;
  background: #f7f7f7;
}
.navList{
  display: flex;
  align-items: center;
  padding:20rpx 0;
}
.navTitle{
  width: 50%;
  display: flex;
  justify-content: center;
  position: relative;
}
.navname{
  font-size: 30rpx;
  color: #101010;
}
.line{
  width: 59rpx;
  height: 4rpx;
  position: absolute;
  background: #aaa;
  bottom:-12rpx;
}
.body{
  width: 92%;
  margin:0 auto;
  margin-top: 8rpx;
}
.orderList{
  width: 100%;
  background: #fff;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
}
.orderList_top{
  display: flex;
  width: 92%;
  margin:0 auto;
  border-bottom: 1px solid #ececec;
  padding:16rpx 0;
}
.orderList_type{
  font-size: 24rpx;
  color: #666;
}
.orderList_number{
  font-size: 24rpx;
  color: #666;
  margin-left: 10rpx;
}
.orderList_mid{
  width: 92%;
  margin:0 auto;
  display: flex;
  justify-content: space-between;
  padding:20rpx 0;
}
.orderList_pic{
  width: 128rpx;
  height: 140rpx;
  background: #e7e7e7;
}
.orderList_pic image{
  max-width: 100%;
  max-height: 100%;
}
.orderList_det{
  display: flex;
  flex:1;
  align-items: center;
  justify-content: space-between;
  margin-left: 34rpx;
}
.orderList_detleft_title{
  font-size: 28rpx;
  color: #101010;
  line-height: 44rpx;
}
.orderList_detleft_type{
  font-size: 24rpx;
  color: #666;
  margin-top: 16rpx;
}
.icon-youjiantou{
  color: #A2A2A2;
  font-size: 24rpx;
}
.orderList_detleft{
  height: 100%;
}
.orderList_bot{
  width: 92%;
  margin:0 auto;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  padding:28rpx 0;
}
.orderList_botbtn{
  width: 136rpx;
  height: 48rpx;
  border-radius: 4rpx;
  background-color: rgba(255, 255, 255, 0);
  border: 1px solid rgba(232, 232, 232, 1);
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 22rpx;
  color: #666;
  margin-left: 32rpx;
}
.stauts{
  width: 104rpx;
  height: 44rpx;
  border-radius: 10rpx;
  background: rgba(234,234,234,1);
  color: #aeaeae;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 30rpx;
}